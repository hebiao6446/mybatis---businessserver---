    "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ProductCategory">

	<resultMap type="ProductCategory" id="ProductCategoryMap">

