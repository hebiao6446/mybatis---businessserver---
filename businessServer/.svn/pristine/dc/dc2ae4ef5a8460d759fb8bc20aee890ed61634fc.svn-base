package com.common;

import java.util.ArrayList;
import java.util.List;

public class PushThread64 extends Thread {
	private ArrayList<String> androidTokens;
	private String content;

	public PushThread64(ArrayList<String> androidTokens, String content) {
		this.setAndroidTokens(androidTokens);
		this.setContent(content);
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getContent() {
		return content;
	}

	public void setAndroidTokens(ArrayList<String> androidTokens) {
		this.androidTokens = androidTokens;
	}

	public ArrayList<String> getAndroidTokens() {
		return androidTokens;
	}

}
